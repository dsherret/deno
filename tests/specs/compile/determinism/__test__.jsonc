{
  "tempDir": true,
  "steps": [{
    "args": "run -A setup.ts",
    "output": "[WILDCARD]"
  }, {
    "if": "unix",
    "args": "compile --no-config --output a a/main.ts",
    "output": "[WILDCARD]"
  }, {
    "if": "windows",
    "args": "compile --no-config --output a.exe a/main.ts",
    "output": "[WILDCARD]"
  }, {
    "args": ["eval", "Deno.renameSync('a', 'b')"],
    "output": "[WILDCARD]"
  }, {
    "if": "unix",
    "args": "compile --no-config --output b b/main.ts",
    "output": "[WILDCARD]"
  }, {
    "if": "windows",
    "args": "compile --no-config --output b.exe b/main.ts",
    "output": "[WILDCARD]"
  }, {
    "if": "unix",
    "args": "run --allow-read=. assert_equal.ts a b",
    "output": "Same\n"
  }, {
    "if": "windows",
    "args": "run --allow-read=. assert_equal.ts a.exe b.exe",
    "output": "Same\n"
  }]
}
